/**app.wxss**/
@import './utils/iconfont/iconfont.wxss';
@import './utils/common/common.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.line2-ellipsis{
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    line-clamp: 2;
    -webkit-box-orient: vertical;
}

.btn-circle{
  width:150rpx;
  height:70rpx;
  line-height: 70rpx;
  background:#108387;
  border-radius:35rpx;
  text-align:center;
  color:#fff;
}